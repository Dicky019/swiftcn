{
  "name": "swiftcn",
  "version": "1.0.2",
  "description": "CLI for swiftcn - shadcn/ui inspired SwiftUI components",
  "type": "module",
  "bin": {
    "swiftcn": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "update": "node scripts/update.js"
  },
  "keywords": [
    "swiftui",
    "components",
    "cli",
    "shadcn"
  ],
  "author": "swiftcn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dicky019/swiftcn.git"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "!dist/__tests__",
    "registry.json"
  ],
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.0.0",
    "simple-git": "^3.24.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.14.0",
    "@vitest/coverage-v8": "^4.0.18",
    "typescript": "^5.4.5",
    "vitest": "^4.0.18"
  }
}
